{
  "name": "shamba-manager",
  "description": "",
  "version": "0.3.0-1",
  "homepage": "",
  "private": true,
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "slilan",
    "email": "slilan96@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^12.0.0",
    "yarn": ">= 0.18.0"
  },
  "scripts": {
    "commit": "git-cz",
    "test": "yarn test:mocha",
    "test:local": "NODE_ENV=test yarn test:mocha",
    "lint": "eslint src/. test/. --config .eslintrc.json",
    "dev": "yarn run:migrations && nodemon src/",
    "start": "node src/",
    "test:mocha": "mocha test/migrations/migrations.test.js test/appTests/**/*.test.js --exit",
    "run:migrations": "knex migrate:latest",
    "release": "standard-version",
    "snyk-protect": "snyk protect",
    "prepare": "yarn run snyk-protect",
    "coverage": "nyc mocha test/ --recursive --exit",
    "rollback": "NODE_ENV=test knex migrate:rollback"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "ignore": []
  },
  "dependencies": {
    "@feathersjs/authentication": "^4.5.1",
    "@feathersjs/authentication-local": "^4.5.1",
    "@feathersjs/authentication-oauth": "^4.5.1",
    "@feathersjs/configuration": "^4.5.1",
    "@feathersjs/errors": "^4.5.1",
    "@feathersjs/express": "^4.5.1",
    "@feathersjs/feathers": "^4.5.1",
    "@feathersjs/transport-commons": "^4.5.3",
    "chai-as-promised": "^7.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "feathers-objection": "^4.8.1",
    "feathers-swagger": "^1.2.1",
    "helmet": "^3.21.2",
    "knex": "^0.21.1",
    "limiter": "^1.1.5",
    "objection": "^2.1.2",
    "pg": "^8.3.0",
    "serve-favicon": "^2.5.0",
    "snyk": "^1.364.0",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "axios": "^0.19.2",
    "chai": "^4.2.0",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "3.2.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "husky": "^4.2.5",
    "mocha": "^7.0.1",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.1",
    "standard-version": "^8.0.2",
    "unexpected": "^11.14.0",
    "unexpected-knex": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "snyk": true
}
